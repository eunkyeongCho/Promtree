   "source": [
    "import os\n",
    "import pymupdf\n",
    "\n",
    "os.makedirs(\"test\", exist_ok=True)\n",
    "\n",
    "doc = pymupdf.open(\"815GD-Brand-Sheet.pdf\")\n",
    "\n",
    "for idx in range(doc.page_count):\n",
    "    page = doc[idx]\n",
    "    pix = page.get_pixmap()\n",
    "    pix.save(f\"test/page_{idx + 1}.jpeg\")\n",
    "\n",
    "doc.close"
   ]
